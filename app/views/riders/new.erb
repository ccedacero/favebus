<h1>Sign Up to Next Bus to Start Tracking Buses</h1>
<%= form_for @rider do |f| %>
<%= f.label :name%><br>
<%= f.text_field :name %><br>
<%= f.label :username%><br>
<%= f.text_field :username%><br>
<%= f.label :password%><br>
<%= f.password_field :password%><br>
<%= f.submit "Create New Account" %>
<% end %>